<!--  scripts for Google Maps -->
<script src="https://maps.googleapis.com/maps/api/js?key=AIzaSyAE03QBe5yDXRr1fzDvkWs9i_E_BIyCDhk&libraries=places" async
 defer></script>
<script src="https://apis.google.com/js/api.js" type="text/javascript"></script>

<style>
	header {
		color: white;
		background-image: linear-gradient(180deg, rgba(224, 133, 14, 1), rgba(173, 52, 62, 1));
	}

	body {
		background-image: url('matrixgridfloor.png');
	}

	.monotonFont {
		font-family: 'Monoton', cursive;
	}
</style>


<h2> Users Listing </h2>
<button class='btn btn-warning' id="btn-log">Audit Log</button>
<button class='btn btn-warning' id="btn-admin-list">Admin List</button>
<button class='btn btn-warning' id="btn-add">Add User</button>
<button class='btn btn-warning' id="btn-admin-physics">Physics Engine</button>
<button class='btn btn-warning' id="btn-mobile-super">Super User</button>
<hr>

<table style="width:100%" class="table table-bordered table-condensed table-hover">
	<tr>
		<strong>
			<th>User id</th>
			<th>User Name</th>
			<th>Email as stored</th>
			<th>Email decrypted</th>
			<th>Password hash</th>
		</strong>
	</tr>
	{{#each outputLines}}
	<tr>
		<td>
			<h5>{{this.id}}</h5>
		</td>
		<td>
			<h5>{{this.name}}</h5>
		</td>
		<td>
			<h5>{{this.email_hash}}</h5>
		</td>
		<td>
			<h5>{{this.email}}</h5>
		</td>
		<td>
			<h5>{{this.password_hash}}</h5>
		</td>
	</tr>
	{{/each}}
</table>



<br/>
<br/>
<br/>
<br/>

<script type="text/javascript" src="../../javascript/numeral.js"></script>



<script>

	$(document).ready(function () {
		//document is ready, so move the incoming data over
		$(document.body).on("click", "#btn-add", function () {
			window.location = "/admin/create";
		});
		$(document.body).on("click", "#btn-log", function () {
			window.location = "/admin/log-disp";
		});
		$(document.body).on("click", "#btn-admin-list", function () {
			window.location = "/admin/admin_list";
		});
		$(document.body).on("click", "#btn-admin-physics", function () {
			window.location = "/physics";
		});
		$(document.body).on("click", "#btn-mobile-super", function () {
			window.location = "/users/mobile/1/1";
		});
		$(document.body).on("click", "#btn-admin-pics", function () {
			window.location = "/admin/files";
		});
	});

</script>